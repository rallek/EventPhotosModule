{# Purpose of this template: Display album items within an external context #}
{% for albumItem in items %}
    <h3>{{ albumItem|rkeventphotosmodule_formattedTitle }}</h3>
    <p><a href="{{ path('rkeventphotosmodule_albumitem_display', {'id': albumItem.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
